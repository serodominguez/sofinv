{"ast":null,"code":"import product from '@/store/modules/product';\nexport default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    },\n    product: {\n      type: Object,\n      default: () => ({\n        pK_PRODUCT: null,\n        code: '',\n        description: '',\n        measurement: '',\n        material: '',\n        colour: '',\n        price: null,\n        pK_CATEGORY: null,\n        pK_BRAND: null\n      })\n    },\n    action: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localProduct: {\n        ...this.product\n      }\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    product: {\n      handler(newProduct) {\n        this.localProduct = {\n          ...newProduct\n        };\n      },\n      deep: true\n    }\n  },\n  methods: {\n    close() {\n      this.isOpen = false;\n      this.$emit('update:modelValue', false);\n    },\n    async activar() {\n      await this.$store.dispatch('product/enabledProduct', this.localProduct.pK_PRODUCT);\n      this.close();\n    },\n    async desactivar() {\n      await this.$store.dispatch('product/disabledProduct', this.localProduct.pK_PRODUCT);\n      this.close();\n    }\n  }\n};","map":{"version":3,"names":["product","props","modelValue","type","Boolean","required","Object","default","pK_PRODUCT","code","description","measurement","material","colour","price","pK_CATEGORY","pK_BRAND","action","Number","data","isOpen","valid","localProduct","watch","newValue","handler","newProduct","deep","methods","close","$emit","activar","$store","dispatch","desactivar"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\Product\\ProductModal.vue"],"sourcesContent":["<template>\r\n    <v-dialog v-model=\"isOpen\" max-width=\"400px\">\r\n      <v-card>\r\n        <v-card-title class=\"headline\" v-if=\"action === 1\">Activar Item?<