   try{\r\n        await this.$store.dispatch('category/enabledCategory', this.localCategory.pK_CATEGORY);\r\n        this.close();\r\n      } catch (error) {\r\n        console.error(\"Error when enabling the category:\", error.message);\r\n        this.toast.error('Error al habilitar la categoría.');\r\n      }\r\n    },\r\n    async disabled() {\r\n      try{\r\n        await this.$store.dispatch('category/disabledCategory', this.localCategory.pK_CATEGORY);\r\n        this.close();\r\n      } catch (error) {\r\n        console.error(\"Error when disabling the category:\", error.message);\r\n        this.toast.error('Error al deshabilitar la categoría.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAsBA,eAAe;EACbA,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,QAAQ,EAAE;MACRH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QACdC,WAAW,EAAE,IAAI;QACjBC,aAAa,EAAE;MACjB,CAAC;IACH,CAAC;IACDC,MAAM,EAAE;MACNR,IAAI,EAAES,MAAM;MACZP,QAAQ,EAAE;IACZ;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAACZ,UAAU;MACvBa,KAAK,EAAE,KAAK;MACZC,aAAa,EAAE;QAAE,GAAG,IAAI,CAACV;MAAS;IACpC,CAAC;EACH,CAAC;EACDW,KAAK,EAAE;IACLf,UAAUA,CAACgB,QAAQ,EAAE;MACnB,IAAI,CAACJ,MAAK,GAAII,QAAQ;IACxB,CAAC;IACDZ,QAAQ,EAAE;MACRa,OAAOA,CAACC,WAAW,EAAE;QACnB,IAAI,CAACJ,aAAY,GAAI;UAAE,GAAGI;QAAY,CAAC;MACzC,CAAC;MACDC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,IAAI,CAACT,MAAK,GAAI,KAAK;MACnB,IAAI,CAACU,KAAK,CAAC,mBAAmB,EAAE,KAAK,CAAC;IACxC,CAAC;IACD,MAAMC,OAAOA,CAAA,EAAG;MACd,IAAG;QACD,MAAM,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,0BAA0B,EAAE,IAAI,CAACX,aAAa,CAACP,WAAW,CAAC;QACtF,IAAI,CAACc,KAAK,CAAC,CAAC;MACd,EAAE,OAAOK,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mCAAmC,EAAEA,KAAK,CAACE,OAAO,CAAC;QACjE,IAAI,CAACC,KAAK,CAACH,KAAK,CAAC,kCAAkC,CAAC;MACtD;IACF,CAAC;IACD,MAAMI,QAAQA,CAAA,EAAG;MACf,IAAG;QACD,MAAM,IAAI,CAACN,MAAM,CAACC,QAAQ,CAAC,2BAA2B,EAAE,IAAI,CAACX,aAAa,CAACP,WAAW,CAAC;QACvF,IAAI,CAACc,KAAK,CAAC,CAAC;MACd,EAAE,OAAOK,KAAK,EAA