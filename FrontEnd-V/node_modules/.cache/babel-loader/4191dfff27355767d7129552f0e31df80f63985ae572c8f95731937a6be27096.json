,\r\n      localStore: { ...this.store },\r\n    };\r\n  },\r\n  watch: {\r\n    modelValue(newValue) {\r\n      this.isOpen = newValue;\r\n    },\r\n    store: {\r\n      handler(newStore) {\r\n        this.localStore = { ...newStore };\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n  methods: {\r\n    close() {\r\n      this.isOpen = false;\r\n      this.$emit('update:modelValue', false);\r\n    },\r\n    async enabled() {\r\n      try{\r\n        await this.$store.dispatch('store/enabledStore', this.localStore.pK_STORE);\r\n        this.toast.success('Marca habilitada con éxito!');\r\n        this.close();\r\n      }catch (error) {\r\n        console.error(\"Error when enabling the store:\", error.message);\r\n        this.toast.error('Error al habilitar la sucursal.');\r\n      }\r\n    },\r\n    async disabled() {\r\n      try{\r\n        await this.$store.dispatch('store/disabledStore', this.localStore.pK_STORE);\r\n        this.toast.success('Marca deshabilitada con éxito!');\r\n        this.close();\r\n      } catch (error) {\r\n        console.error(\"Error when disabling the store:\", error.message);\r\n        this.toast.error('Error al deshabilitar la sucursal.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAsBA,SAASA,QAAO,QAAS,oBAAoB;AAC7C,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,KAAK,EAAE;MACLH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QACdC,QAAQ,EAAE,IAAI;QACdC,UAAU,EAAE,EAAE;QACdC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT,CAAC;IACH,CAAC;IACDC,MAAM,EAAE;MACNb,IAAI,EAAEc,MAAM;MACZZ,QAAQ,EAAE;IACZ;EACF,CAAC;EACDa,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAACjB,UAAU;MACvBkB,KAAK,EAAE,KAAK;MACZC,KAAK,EAAErB,QAAQ,CAAC,CAAC;MACjBsB,UAAU,EAAE;QAAE,GAAG,IAAI,CAAChB;MAAM;IAC9B,CAAC;EACH,CAAC;EACDiB,KAAK,EAAE;IACLrB,UAAUA,CAACsB,QAAQ,EAAE;MACnB,IAAI,CAACL,MAAK,GAAIK,QAAQ;IACxB,CAAC;IACDlB,KAAK,EAAE;MACLmB,OAAOA,CAACC,QAAQ,E