{"ast":null,"code":"export default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    },\n    receipt: {\n      type: Object,\n      default: () => ({\n        pK_RECEIPT: null,\n        code: ''\n      })\n    },\n    action: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localReceipt: {\n        ...this.receipt\n      }\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    receipt: {\n      handler(newReceipt) {\n        this.localReceipt = {\n          ...newReceipt\n        };\n      },\n      deep: true\n    }\n  },\n  methods: {\n    close() {\n      this.isOpen = false;\n      this.$emit('update:modelValue', false);\n    },\n    async disabled() {\n      await this.$store.dispatch('goodsreceipt/disabledReceipt', this.localReceipt.pK_RECEIPT);\n      this.close();\n    }\n  }\n};","map":{"version":3,"names":["props","modelValue","type","Boolean","required","receipt","Object","default","pK_RECEIPT","code","action","Number","data","isOpen","valid","localReceipt","watch","newValue","handler","newReceipt","deep","methods","close","$emit","disabled","$store","dispatch"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\GoodsReceipt\\ReceiptAction.vue"],"sourcesContent":["<template>\r\n  <v-dialog v-model=\"isOpen\" max-width=\"400px\" persistent>\r\n    <v-card>\r\n      <v-card-title class=\"bg-surface-light pt-4\">Desactivar Item?</v-card-title>\r\n      <v-divider></v-divider>\r\n      <v-card-text>\r\n        Estás a punto de Desactivar el ítem {{ localReceipt.code }}.\r\n      </v-card-text>\r\n      <v-card-actions class=\"d-flex justify-space-between\">\r\n        <div class=\"d-flex\">\r\n          <v-btn color=\"blue\" dark class=\"mr-2\" elevation=\"4\" @click=\"disabled\">Desactivar</v-btn>\r\n          <v-btn color=\"red\" elevation=\"4\" @click=\"close\">Cancelar</v-btn>\r\n        </div>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    modelValue: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n    receipt: {\r\n      type: Object,\r\n      default: () => ({ pK_RECEIPT: null, code: '' }),\r\n    },\r\n    action: {\r\n      type: Number,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      isOpen: this.modelValue,\r\n      valid: false,\r\n      localReceipt: { ...this.receipt },\r\n    };\r\n  },\r\n  watch: {\r\n    modelValue(newValue) {\r\n      this.isOpen = newValue;\r\n    },\r\n    receipt: {\r\n      handler(newReceipt) {\r\n        this.localReceipt = { ...newReceipt };\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n  methods: {\r\n    close() {\r\n      this.isOpen = false;\r\n      this.$emit('update:modelValue', false);\r\n    },\r\n    async disabled() {\r\n      await this.$store.dispatch('goodsreceipt/disabledReceipt', this.localReceipt.pK_RECEIPT);\r\n      this.close();\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAmBA,eAAe;EACbA,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QAAEC,UAAU,EAAE,IAAI;QAAEC,IAAI,EAAE;MAAG,CAAC;IAChD,CAAC;IACDC,MAAM,EAAE;MACNR,IAAI,EAAES,MAAM;MACZP,QAAQ,EAAE;IACZ;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAACZ,UAAU;MACvBa,KAAK,EAAE,KAAK;MACZC,YAAY,EAAE;QAAE,GAAG,IAAI,CAACV;MAAQ;IAClC,CAAC;EACH,CAAC;EACDW,KAAK,EAAE;IACLf,UAAUA,CAACgB,QAAQ,EAAE;MACnB,IAAI,CAACJ,MAAK,GAAII,QAAQ;IACxB,CAAC;IACDZ,OAAO,EAAE;MACPa,OAAOA,CAACC,UAAU,EAAE;QAClB,IAAI,CAACJ,YAAW,GAAI;UAAE,GAAGI;QAAW,CAAC;MACvC,CAAC;MACDC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,IAAI,CAACT,MAAK,GAAI,KAAK;MACnB,IAAI,CAACU,KAAK,CAAC,mBAAmB,EAAE,KAAK,CAAC;IACxC,CAAC;IACD,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAM,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,8BAA8B,EAAE