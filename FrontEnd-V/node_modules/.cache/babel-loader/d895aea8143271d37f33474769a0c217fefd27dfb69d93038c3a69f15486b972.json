derlined\" v-model=\"localUser.surnames\"\r\n                  :rules=\"[rules.required]\" :maxlength=\"30\" @keyup=\"uppercase\" label=\"Apellidos\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.identification\"\r\n                   label=\"Nº de Carnet\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.phone\"\r\n                   label=\"Teléfono\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-autocomplete color=\"primary\" variant=\"underlined\" :items=\"roles\"\r\n                  v-model=\"localProduct.pK_ROLE\" item-title=\"rolE_NAME\" item-value=\"pK_ROLE\" :rules=\"[rules.required]\"\r\n                  no-data-text=\"No hay datos disponibles\" label=\"Rol\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-autocomplete color=\"primary\" variant=\"underlined\" :items=\"stores\"\r\n                  v-model=\"localProduct.pK_STORE\" item-title=\"storE_NAME\" item-value=\"pK_STORE\" :rules=\"[rules.required]\"\r\n                  no-data-text=\"No hay datos disponibles\" label=\"Tienda\" required />\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-col xs12 sm12 md12 lg12 xl12>\r\n        <v-card-actions>\r\n          <v-btn color=\"blue\" dark class=\"mb-2\" elevation=\"4\" @click=\"saveUser\" :disabled=\"!valid\">Guardar</v-btn>\r\n          <v-btn color=\"red\" dark class=\"mb-2\" elevation=\"4\" @click=\"close\">Cancelar</v-btn>\r\n        </v-card-actions>\r\n      </v-col>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>