{"ast":null,"code":"export default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    },\n    user: {\n      type: Object,\n      default: () => ({\n        pK_USER: null,\n        useR_NAME: '',\n        passworD_HASH: '',\n        names: '',\n        surnames: '',\n        identification: '',\n        phone: '',\n        pK_ROLE: '',\n        pK_STORE: ''\n      })\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localUser: {\n        ...this.user\n      },\n      rules: {\n        required: value => !!value || 'Este campo es requerido.'\n      }\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    isOpen(newValue) {\n      this.$emit('update:modelValue', newValue);\n    },\n    user: {\n      handler(newUser) {\n        this.localUser = {\n          ...newUser\n        };\n      },\n      deep: true\n    }\n  },\n  mounted() {\n    this.$store.dispatch('role/selectRoles');\n    this.$store.dispatch('store/selectStores');\n  },\n  methods: {\n    uppercase() {\n      this.localUser.useR_NAME = this.localUser.useR_NAME.toUpperCase();\n      this.localUser.names = this.localUser.names.toUpperCase();\n      this.localUser.surnames = this.localUser.surnames.toUpperCase();\n    },\n    close() {\n      this.isOpen = false;\n    },\n    async saveUser() {\n      if (this.$refs.form.validate()) {\n        try {\n          if (this.localUser.pK_USER) {\n            await this.$store.dispatch('user/updateUser', {\n              ...this.localUser\n            });\n          } else {\n            await this.$store.dispatch('user/createUser', {\n              ...this.localUser\n            });\n          }\n          this.$emit('saved', {\n            ...this.localUser\n          });\n          this.close();\n        } catch (error) {\n          console.error('Error al guardar el usuario:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":