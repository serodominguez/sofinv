{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport BrandForm from './BrandForm.vue';\nimport BrandModal from './BrandModal.vue';\nexport default {\n  components: {\n    BrandForm,\n    BrandModal\n  },\n  data() {\n    return {\n      items: [5, 10, 25],\n      pages: \"Marcas por Página\",\n      search: null,\n      modal: false,\n      changeStatusModalOpen: false,\n      selectedBrand: null\n    };\n  },\n  computed: {\n    ...mapGetters('brand', ['brands', 'loading']),\n    headers() {\n      return [{\n        title: 'Marca',\n        key: 'branD_NAME'\n      }, {\n        title: 'Estado',\n        key: 'status'\n      }, {\n        title: 'Acciones',\n        key: 'actions',\n        sortable: false\n      }];\n    }\n  },\n  methods: {\n    initialize() {},\n    ...mapActions('brand', ['fetchBrands', 'deleteBrand']),\n    openModal() {\n      this.selectedBrand = {\n        pK_BRAND: null,\n        branD_NAME: ''\n      };\n      this.modal = true;\n    },\n    editBrand(brand) {\n      this.selectedBrand = {\n        ...brand\n      }; // Clonamos el objeto para evitar mutaciones directas\n      this.modal = true;\n    },\n    async deleteBrand(id) {\n      const confirmDelete = confirm('¿Estás seguro de que deseas eliminar esta marca?');\n      if (confirmDelete) {\n        await this.deleteBrand(id);\n      }\n    },\n    openChangeStatusModal(brand) {\n      this.selectedBrand = {\n        ...brand\n      };\n      this.changeStatusModalOpen = true;\n    },\n    closeChangeStatusModal() {\n      this.changeStatusModalOpen = false;\n    },\n    async changeBrandStatus({\n      id,\n      status\n    }) {\n      if (status === 'ACTIVO') {\n        await this.enabledBrand(id);\n      } else {\n        await this.disabledBrand(id);\n      }\n      this.fetchBrands();\n    }\n  },\n  mounted() {\n    this.fetchBrands();\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","BrandForm","BrandModal","components","data","items",