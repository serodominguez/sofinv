{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport ProductForm from './ProductForm.vue';\nimport ProductAction from './ProductAction.vue';\nexport default {\n  components: {\n    ProductForm,\n    ProductAction\n  },\n  data() {\n    return {\n      items: [5, 10, 25],\n      pages: \"Productos por Página\",\n      search: null,\n      form: false,\n      modal: false,\n      selectedProduct: null,\n      action: 0\n    };\n  },\n  computed: {\n    ...mapGetters('product', ['products', 'loading']),\n    headers() {\n      return [{\n        title: 'Código',\n        key: 'code'\n      }, {\n        title: 'Descripción',\n        key: 'description',\n        sortable: false\n      }, {\n        title: 'Medida',\n        key: 'measurement',\n        sortable: false\n      }, {\n        title: 'Material',\n        key: 'material',\n        sortable: false\n      }, {\n        title: 'Color',\n        key: 'colour',\n        sortable: false\n      }, {\n        title: 'Categoría',\n        key: 'categoria',\n        sortable: false\n      }, {\n        title: 'Marca',\n        key: 'Marca',\n        sortable: false\n      }, {\n        title: 'Agregado',\n        key: 'creatioN_DATE',\n        sortable: false\n      }, {\n        title: 'Estado',\n        key: 'status',\n        sortable: false\n      }, {\n        title: 'Acciones',\n        key: 'actions',\n        sortable: false\n      }];\n    }\n  },\n  methods: {\n    initialize() {\n      this.$store.dispatch('product/fetchProducts');\n      this.search = null;\n    },\n    ...mapActions('product', ['fetchProducts']),\n    openForm() {\n      this.selectedProduct = {\n        pK_PRODUCT: null,\n        code: '',\n        description: '',\n        measurement: '',\n        material: '',\n        colour: '',\n        pK_CATEGORY: null,\n        pK_BRAND: null\n      };\n      this.form = true;\n    },\n    editProduct(product) {\n      this.selectedProduct = {\n        ...product\n      };\