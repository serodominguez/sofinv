{"ast":null,"code":"import { mapGetters } from 'vuex';\nexport default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true,\n      oldPassword: ''\n    },\n    user: {\n      type: Object,\n      default: () => ({\n        pK_USER: null,\n        useR_NAME: '',\n        passworD_HASH: '',\n        names: '',\n        surnames: '',\n        identification: '',\n        phone: '',\n        pK_ROLE: '',\n        pK_STORE: ''\n      })\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localUser: {\n        ...this.user\n      },\n      rules: {\n        required: value => !!value || 'Este campo es requerido.'\n      }\n    };\n  },\n  computed: {\n    ...mapGetters('role', ['roles']),\n    ...mapGetters('store', ['stores'])\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    isOpen(newValue) {\n      this.$emit('update:modelValue', newValue);\n    },\n    user: {\n      handler(newUser) {\n        this.localUser = {\n          ...newUser\n        };\n        if (newUser.pK_USER) {\n          this.oldPassword = newUser.passworD_HASH;\n        } else {\n          this.oldPassword = '';\n        }\n      },\n      deep: true\n    }\n  },\n  mounted() {\n    this.$store.dispatch('role/selectRoles');\n    this.$store.dispatch('store/selectStores');\n  },\n  methods: {\n    uppercase() {\n      this.localUser.useR_NAME = this.localUser.useR_NAME.toUpperCase();\n      this.localUser.names = this.localUser.names.toUpperCase();\n      this.localUser.surnames = this.localUser.surnames.toUpperCase();\n    },\n    close() {\n      this.isOpen = false;\n    },\n    async saveUser() {\n      if (this.$refs.form.validate()) {\n        try {\n          if (this.localUser.pK_USER) {\n            if (this.localUser.passworD_HASH != this.oldPassword) {\n              const userData = {\n                pK_USER: this.localUser.pK_USER,\n                useR_NAME: this.localUser.useR_NAME,\n                password: this.localUser.passworD_HASH,\n                names: this.localUser.names,\n                surnames: this.localUser.surnames,\n                identification: this.localUser.identification,\n                phone: this.localUser.phone,\n                pK_ROLE: this.localUser.pK_ROLE,\n                pK_STORE: this.localUser.pK_STORE,\n                updatePassword: true\n              };\n              await this.$store.dispatch('user/updateUser', {\n                userData\n              });\n            } else {\n              const userData = {\n                pK_USER: this.localUser.pK_USER,\n                useR_NAME: this.localUser.useR_NAME,\n                names: this.localUser.names,\n                surnames: this.localUser.surnames,\n                identification: this.localUser.identification,\n                phone: this.localUser.phone,\n                pK_ROLE: this.localUser.pK_ROLE,\n                pK_STORE: this.localUser.pK_STORE,\n                updatePassword: false\n              };\n              await this.$store.dispatch('user/updateUser', {\n                userData\n              });\n            }\n          } else {\n            await this.$store.dispatch('user/createUser', {\n              ...this.localUser\n            });\n          }\n          this.$emit('saved', {\n            ...this.localUser\n          });\n          this.close();\n        } catch (error) {\n          console.error('Error al guardar el usuario:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","props","modelValue","type","Boolean","required","oldPassword","user","Object","default","pK_USER","useR_NAME","passworD_HASH","names","surnames","identification","phone","pK_ROLE","pK_STORE","data","isOpen","valid","localUser","rules","value","computed","watch","newValue","$emit","handler","newUser","deep","mounted","$store","dispatch","methods","uppercase","toUpperCase","close","saveUser","$refs","form","validate","userData","password","updatePassword","error","console"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\User\\UserForm.vue"],"sourcesContent":["<template>\r\n  <v-dialog v-model=\"isOpen\" max-width=\"500px\" persistent>\r\n    <v-card>\r\n      <v-card-title class=\"bg-surface-light pt-4\">\r\n        <span>{{ localUser.pK_USER ? 'Editar Usuario' : 'Agregar Usuario' }}</span>\r\n      </v-card-title>\r\n      <v-divider></v-divider>\r\n      <v-card-text>\r\n        <v-form ref=\"form\" v-model=\"valid\">\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.useR_NAME\"\r\n                  :rules=\"[rules.required]\" :maxlength=\"25\" @keyup=\"uppercase\" label=\"Nombre de Usuario\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.passworD_HASH\" type=\"password\"\r\n                  :rules=\"[rules.required]\" label=\"Contraseña\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.names\"\r\n                  :rules=\"[rules.required]\" :maxlength=\"30\" @keyup=\"uppercase\" label=\"Nombres\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.surnames\"\r\n                  :rules=\"[rules.required]\" :maxlength=\"30\" @keyup=\"uppercase\" label=\"Apellidos\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.identification\"\r\n                   label=\"Nº de Carnet\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localUser.phone\"\r\n                   label=\"Teléfono\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-autocomplete color=\"primary\" variant=\"underlined\" :items=\"roles\"\r\n                  v-model=\"localUser.pK_ROLE\" item-title=\"rolE_NAME\" item-value=\"pK_ROLE\" :rules=\"[rules.required]\"\r\n                  no-data-text=\"No hay datos disponibles\" label=\"Rol\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-autocomplete color=\"primary\" variant=\"underlined\" :items=\"stores\"\r\n                  v-model=\"localUser.pK_STORE\" item-title=\"storE_NAME\" item-value=\"pK_STORE\" :rules=\"[rules.required]\"\r\n                  no-data-text=\"No hay datos disponibles\" label=\"Tienda\" required />\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-col xs12 sm12 md12 lg12 xl12>\r\n        <v-card-actions>\r\n          <v-btn color=\"blue\" dark class=\"mb-2\" elevation=\"4\" @click=\"saveUser\" :disabled=\"!valid\">Guardar</v-btn>\r\n          <v-btn color=\"red\" dark class=\"mb-2\" elevation=\"4\" @click=\"close\">Cancelar</v-btn>\r\n        </v-card-actions>\r\n      </v-col>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nexport default {\r\n  props: {\r\n    modelValue: {\r\n      type: Boolean,\r\n      required: true,\r\n      oldPassword: '',\r\n    },\r\n    user: {\r\n      type: Object,\r\n      default: () => ({ pK_USER: null, useR_NAME: '', passworD_HASH: '', names: '', surnames: '', identification: '', phone: '', pK_ROLE: '', pK_STORE: '' }),\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      isOpen: this.modelValue,\r\n      valid: false,\r\n      localUser: { ...this.user },\r\n      rules: {\r\n        required: (value) => !!value || 'Este campo es requerido.',\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters('role', ['roles']),\r\n    ...mapGetters('store', ['stores']),\r\n  },\r\n  watch: {\r\n    modelValue(newValue) {\r\n      this.isOpen = newValue;\r\n    },\r\n    isOpen(newValue) {\r\n      this.$emit('update:modelValue', newValue);\r\n    },\r\n    user: {\r\n      handler(newUser) {\r\n        this.localUser = { ...newUser };\r\n        if (newUser.pK_USER) {\r\n          this.oldPassword = newUser.passworD_HASH;\r\n        } else {\r\n          this.oldPassword = ''; \r\n        }\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$store.dispatch('role/selectRoles');\r\n    this.$store.dispatch('store/selectStores');\r\n  },\r\n  methods: {\r\n    uppercase() {\r\n      this.localUser.useR_NAME = this.localUser.useR_NAME.toUpperCase();\r\n      this.localUser.names = this.localUser.names.toUpperCase();\r\n      this.localUser.surnames = this.localUser.surnames.toUpperCase();\r\n    },\r\n    close() {\r\n      this.isOpen = false;\r\n    },\r\n    async saveUser() {\r\n      if (this.$refs.form.validate()) {\r\n        try {\r\n          if (this.localUser.pK_USER) {\r\n            if (this.localUser.passworD_HASH != this.oldPassword) {\r\n              const userData = {\r\n                pK_USER: this.localUser.pK_USER,\r\n                useR_NAME: this.localUser.useR_NAME,\r\n                password: this.localUser.passworD_HASH,\r\n                names: this.localUser.names,\r\n                surnames: this.localUser.surnames,\r\n                identification: this.localUser.identification,\r\n                phone: this.localUser.phone,\r\n                pK_ROLE: this.localUser.pK_ROLE,\r\n                pK_STORE: this.localUser.pK_STORE,\r\n                updatePassword: true,\r\n              };\r\n              await this.$store.dispatch('user/updateUser', { userData });\r\n            } else {\r\n              const userData = {\r\n                pK_USER: this.localUser.pK_USER,\r\n                useR_NAME: this.localUser.useR_NAME,\r\n                names: this.localUser.names,\r\n                surnames: this.localUser.surnames,\r\n                identification: this.localUser.identification,\r\n                phone: this.localUser.phone,\r\n                pK_ROLE: this.localUser.pK_ROLE,\r\n                pK_STORE: this.localUser.pK_STORE,\r\n                updatePassword: false,\r\n              };\r\n              await this.$store.dispatch('user/updateUser', { userData });\r\n            }\r\n          } else {\r\n            await this.$store.dispatch('user/createUser', { ...this.localUser });\r\n          }\r\n          this.$emit('saved', { ...this.localUser });\r\n          this.close();\r\n        } catch (error) {\r\n          console.error('Error al guardar el usuario:', error);\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AA4DA,SAASA,UAAS,QAAS,MAAM;AACjC,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE,IAAI;MACdC,WAAW,EAAE;IACf,CAAC;IACDC,IAAI,EAAE;MACJJ,IAAI,EAAEK,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QAAEC,OAAO,EAAE,IAAI;QAAEC,SAAS,EAAE,EAAE;QAAEC,aAAa,EAAE,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE,EAAE;QAAEC,cAAc,EAAE,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,CAAC;IACxJ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAAClB,UAAU;MACvBmB,KAAK,EAAE,KAAK;MACZC,SAAS,EAAE;QAAE,GAAG,IAAI,CAACf;MAAK,CAAC;MAC3BgB,KAAK,EAAE;QACLlB,QAAQ,EAAGmB,KAAK,IAAK,CAAC,CAACA,KAAI,IAAK;MAClC;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGzB,UAAU,CAAC,MAAM,EAAE,CAAC,OAAO,CAAC,CAAC;IAChC,GAAGA,UAAU,CAAC,OAAO,EAAE,CAAC,