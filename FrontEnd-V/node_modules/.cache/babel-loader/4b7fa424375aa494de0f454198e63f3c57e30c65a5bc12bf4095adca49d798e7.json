{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nexport default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      items: [5, 10, 25],\n      pages: \"Productos por Página\",\n      search: null,\n      selectedProduct: null,\n      isOpen: this.modelValue,\n      valid: false\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    }\n  },\n  computed: {\n    ...mapGetters('warehouse', ['warehouses', 'loading']),\n    headers() {\n      return [{\n        title: 'Código',\n        key: 'code'\n      }, {\n        title: 'Descripción',\n        key: 'description',\n        sortable: false\n      }, {\n        title: 'Material',\n        key: 'material',\n        sortable: false\n      }, {\n        title: 'Color',\n        key: 'colour',\n        sortable: false\n      }, {\n        title: 'Cantidad',\n        key: 'stock',\n        sortable: false\n      }, {\n        title: 'Precio',\n        key: 'price',\n        sortable: false\n      }, {\n        title: 'Categoría',\n        key: 'categoria',\n        sortable: false\n      }, {\n        title: 'Marca',\n        key: 'Marca',\n        sortable: false\n      }, {\n        title: 'Acciones',\n        key: 'actions',\n        sortable: false\n      }];\n    }\n  },\n  methods: {\n    initialize() {\n      this.search = null;\n    },\n    ...mapActions('warehouse', ['fetchStock']),\n    openForm() {\n      this.selectedProduct = {\n        pK_PRODUCT: null,\n        code: '',\n        description: '',\n        material: '',\n        colour: '',\n        stock: null,\n        price: null,\n        branD_NAME: '',\n        categorY_NAME: ''\n      };\n      this.form = true;\n    },\n    addroduct(product) {\n      this.selectedProduct = {\n        ...product\n      };\n    },\n    fetchStock() {\n      this.$store.dispatch('warehouse/fetchStock', this.search, this.$store.state.currentUser.pk_store);\n    },\n    close() {\n      this.isOpen = false;\n      this.$emit('update:modelValue', false);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","props","modelValue","type","Boolean","required","data","items","pages","search","selectedProduct","isOpen","valid","watch","newValue","computed","headers","title","key","sortable","methods","initialize","openForm","pK_PRODUCT","code","description","material","colour","stock","price","branD_NAME","categorY_NAME","form","addroduct","product","fetchStock","$store","dispatch","state","currentUser","pk_store","close","$emit"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\Product\\ProductModal.vue"],"sourcesContent":["<template>\r\n  <v-dialog v-model=\"isOpen\" max-width=\"1200px\" persistent>\r\n    <v-card elevation=\"2\">\r\n      <v-card-text>\r\n        <v-data-table \r\n        :headers=\"headers\" \r\n        :items=\"warehouses\" \r\n        :search=\"search\" \r\n        :items-per-page-text=\"pages\"\r\n        :items-per-page-options=\"[5, 10, 20]\" \r\n        :items-per-page=\"5\" :loading=\"loading\"\r\n        loading-text=\"Cargando... Espere por favor\" page-text=\"{0}-{1} de {2}\">\r\n          <template v-slot:item=\"{ item }\">\r\n            <tr>\r\n              <td>{{ item.code }}</td>\r\n              <td>{{ item.description }}</td>\r\n              <td>{{ item.measurement }}</td>\r\n              <td>{{ item.material }}</td>\r\n              <td>{{ item.colour }}</td>\r\n              <td>{{ item.stock }}</td>\r\n              <td>{{ item.price }}</td>\r\n              <td>{{ item.categorY_NAME }}</td>\r\n              <td>{{ item.branD_NAME }}</td>\r\n              <td>\r\n                <v-icon class=\"me-2\" size=\"small\">add</v-icon>\r\n              </td>\r\n            </tr>\r\n          </template>\r\n          <template v-slot:top>\r\n            <v-toolbar>\r\n              <v-toolbar-title>Productos</v-toolbar-title>\r\n           