Form() {\r\n      this.selectedProduct = {\r\n        pK_PRODUCT: null,\r\n        code: '',\r\n        description: '',\r\n        material: '',\r\n        colour: '',\r\n        stock: null,\r\n        price: null,\r\n        branD_NAME: '',\r\n        categorY_NAME: ''\r\n      };\r\n      this.form = true;\r\n    },\r\n    addroduct(product) {\r\n      this.selectedProduct = { ...product };\r\n    },\r\n    fetchStock() {\r\n      this.$store.dispatch('warehouse/fetchStock', this.search, this.$store.state.currentUser.pk_store);\r\n    },\r\n    close() {\r\n      this.isOpen = false;\r\n      this.$emit('update:modelValue', false);\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAgDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,CAAC;MAClBC,KAAK,EAAE,sBAAsB;MAC7BC,MAAM,EAAE,IAAI;MACZC,eAAe,EAAE,IAAI;MACrBC,MAAM,EAAE,IAAI,CAACT,UAAU;MACvBU,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLX,UAAUA,CAACY,QAAQ,EAAE;MACnB,IAAI,CAACH,MAAK,GAAIG,QAAQ;IACxB;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGhB,UAAU,CAAC,WAAW,EAAE,CAAC,YAAY,EAAE,SAAS,CAAC,CAAC;IACrDiB,OAAOA,CAAA,EAAG;MACR,OAAO,CACL;QAAEC,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE;MAAO,CAAC,EAChC;QAAED,KAAK,EAAE,aAAa;QAAEC,GAAG,EAAE,aAAa;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC7D;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,UAAU;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACvD;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,QAAQ;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAClD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACpD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAClD;QAAEF,KAAK,EAAE,WAAW;QAAEC,GAAG,EAAE,WAAW;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACzD;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACjD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,SAAS;QAAEC,QAAQ,EAAE;MAAM,CAAC,CACvD;IACH;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACZ,MAAK,GAAI,IAAI;IACpB,CAAC;I