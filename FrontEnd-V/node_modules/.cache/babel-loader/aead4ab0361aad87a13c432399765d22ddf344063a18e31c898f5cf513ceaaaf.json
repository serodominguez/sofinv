{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.every.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport \"core-js/modules/esnext.iterator.some.js\";\nimport { mapGetters } from 'vuex';\nimport ProductModal from '../Product/ProductModal.vue';\nimport { jsPDF } from 'jspdf';\nimport { autoTable } from 'jspdf-autotable';\nexport default {\n  components: {\n    ProductModal\n  },\n  props: {\n    modelValue: {\n      type: Boolean,\n      default: false\n    },\n    receipt: {\n      type: Object,\n      default: () => ({\n        pK_RECEIPT: null,\n        code: '',\n        datE_PURCHASE: null,\n        datE_REGISTRATION: '',\n        typE_RECEIPT: '',\n        receipT_NUMBER: '',\n        annotations: '',\n        pK_SUPPLIER: null,\n        pK_USER: null,\n        pK_STORE: null\n      })\n    },\n    receiptDetails: {\n      type: Array,\n      default: () => []\n    }\n  },\n  emits: ['update:modelValue', 'saved', 'close'],\n  data() {\n    return {\n      details: [],\n      itemCounter: 1,\n      items: [5, 10, 25],\n      pages: \"Productos por Página\",\n      isOpen: this.modelValue,\n      alert: false,\n      valid: false,\n      modal: false,\n      warningMessage: '',\n      localReceipt: {\n        ...this.receipt\n      },\n      rules: {\n        required: value => !!value || 'Este campo es requerido.'\n      },\n      documentTypes: [],\n      typesPurchases: ['FACTURA', 'RECIBO'],\n      typesImports: ['DUI'],\n      receiptTypes: ['ADQUISICIÓN', 'IMPORTACIÓN']\n    };\n  },\n  computed: {\n    ...mapGetters('goodsreceipt', ['receipts']),\n    ...mapGetters('supplier', ['suppliers']),\n    headers() {\n      const baseHeaders = [{\n        title: 'Código',\n        key: 'code',\n        sortable: false\n      }, {\n        title: 'Descripción',\n        key: 'description',\n        sortable: false\n      }, {\n        title: 'Material',\n        key: 'material',\n        sortable: false\n      }, {\n        title: 'Color',\n        key: 'colour',\n        sortable: false\n      }, {\n        title: 'Categoría',\n        key: 'categorY_NAME',\n        sortable: false\n      }, {\n        title: 'Marca',\n        key: 'branD_NAME',\n        sortable: false\n      }, {\n        title: 'Cantidad',\n        key: 'quantity',\n        sortable: false\n      }, {\n        title: 'Costo',\n        key: 'cost',\n        sortable: false\n      }];\n      if (this.localReceipt.pK_RECEIPT == null) {\n        baseHeaders.push({\n          title: 'Borrar',\n          key: 'actions',\n          sortable: false\n        });\n      }\n      return baseHeaders;\n    },\n    totalCost() {\n      return this.details.reduce((total, item) => total + (item.cost || 0), 0);\n    }\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    isOpen(newValue) {\n      this.$emit('update:modelValue', newValue);\n    },\n    receipt: {\n      handler(newReceipt) {\n        this.localReceipt = {\n          ...newReceipt\n        };\n        this.details = this.receiptDetails;\n      },\n      deep: true\n    }\n  },\n  methods: {\n    printReceipt() {\n      this.date = new Date().toLocaleString();\n      const doc = new jsPDF('p', 'pt', 'letter');\n      const FONT_SIZE_TITLE = 10;\n      const FONT_SIZE_SUBTITLE = 9;\n      const TITLE_Y_POSITION = 35;\n      const SUBTITLE_Y_POSITION = 50;\n      const CODE_Y_POSITION = 75;\n      const ENTRY_TYPE_Y_POSITION = 95;\n      const TABLE_START_Y = 120;\n      const PAGE_FOOTER_Y_POSITION = 760;\n\n      // Function to set title\n      const setTitle = (text, y) => {\n        doc.setFontSize(FONT_SIZE_TITLE);\n        doc.setFont('Helvetica', 'bold');\n        doc.text(text, 230, y);\n      };\n\n      // Function to set subtitle\n      const setSubtitle = (label, value, y) => {\n        doc.setFontSize(FONT_SIZE_SUBTITLE);\n        doc.setFont('Helvetica', 'bold');\n        doc.text(label, 20, y);\n        doc.setFont('Helvetica', 'normal');\n        doc.text(value, 57 + doc.getTextWidth(label), y);\n      };\n\n      // Set titles and subtitles\n      setTitle(\"CENTROS OPTICOS\", TITLE_Y_POSITION);\n      setTitle(\"ENTRADA A BODEGA\", SUBTITLE_Y_POSITION);\n      setSubtitle(\"Código: \", this.localReceipt.code, CODE_Y_POSITION);\n      setSubtitle(\"Fecha de impresión: \", this.date, CODE_Y_POSITION);\n      setSubtitle(\"Tipo de entrada: \", this.localReceipt.typE_RECEIPT, ENTRY_TYPE_Y_POSITION);\n      setSubtitle(\"Número de comprobante: \", this.localReceipt.documenT_NUMBER, ENTRY_TYPE_Y_POSITION);\n\n      // Prepare data for the table\n      const rows = this.details.map(x => ({\n        code: x.code,\n        description: x.description,\n        material: x.material,\n        colour: x.colour,\n        categorY_NAME: x.categorY_NAME,\n        branD_NAME: x.branD_NAME,\n        quantity: x.quantity,\n        cost: x.cost\n      }));\n\n      // Generate the table\n      autoTable(doc, {\n        columns: [{\n          header: \"Código\",\n          dataKey: \"code\"\n        }, {\n          header: \"Descripción\",\n          dataKey: \"description\"\n        }, {\n          header: \"Material\",\n          dataKey: \"material\"\n        }, {\n          header: \"Color\",\n          dataKey: \"colour\"\n        }, {\n          header: \"Categoría\",\n          dataKey: \"categorY_NAME\"\n        }, {\n          header: \"Marca\",\n          dataKey: \"branD_NAME\"\n        }, {\n          header: \"Cantidad\",\n          dataKey: \"quantity\"\n        }, {\n          header: \"Costo\",\n          dataKey: \"cost\"\n        }],\n        body: rows,\n        startY: TABLE_START_Y,\n        margin: {\n          horizontal: 20\n        },\n        styles: {\n          overflow: \"linebreak\",\n          lineWidth: 0.5,\n          lineColor: [0, 0, 0]\n        },\n        bodyStyles: {\n          textColor: [33, 33, 33],\n          fillColor: [255, 255, 255],\n          font: 'helvetica',\n          valign: \"top\",\n          fontSize: 8\n        },\n        headStyles: {\n          lineWidth: 0.5,\n          lineColor: [0, 0, 0],\n          halign: 'center'\n        },\n        footStyles: {\n          fillColor: [255, 255, 255],\n          textColor: [33, 33, 33],\n          halign: 'left'\n        },\n        showHead: \"everyPage\",\n        showFoot: \"lastPage\",\n        columnStyles: {\n          quantity: {\n            halign: 'right'\n          },\n          cost: {\n            halign: 'right'\n          }\n        }\n      });\n\n      // Add page numbers\n      const pageCount = doc.internal.getNumberOfPages();\n      doc.setFontSize(FONT_SIZE_TITLE);\n      for (let i = 1; i <= pageCount; i++) {\n        doc.setPage(i);\n        doc.text('Página ' + String(i) + ' de ' + String(pageCount), 525, PAGE_FOOTER_Y_POSITION);\n      }\n\n      // Set document properties\n      doc.setProperties({\n        title: \"Entrada a bodega\"\n      });\n      window.open(doc.output('bloburl'));\n    },\n    async saveReceipt() {\n      if (this.details.length === 0) {\n        this.alert = true;\n        return;\n      }\n      const isFormValid = this.$refs.form.validate();\n      const requiredFields = [this.localReceipt.typE_RECEIPT, this.localReceipt.typE_DOCUMENT, this.localReceipt.datE_PURCHASE, this.localReceipt.documenT_NUMBER, this.localReceipt.pK_SUPPLIER];\n      const allFieldsValid = requiredFields.every(field => !!field);\n      if (!isFormValid || !allFieldsValid) {\n        return;\n      }\n      try {\n        const receiptData = {\n          datE_PURCHASE: this.localReceipt.datE_PURCHASE,\n          typE_RECEIPT: this.localReceipt.typE_RECEIPT,\n          typE_DOCUMENT: this.localReceipt.typE_DOCUMENT,\n          documenT_NUMBER: this.localReceipt.documenT_NUMBER,\n          annotations: this.localReceipt.annotations,\n          pK_SUPPLIER: this.localReceipt.pK_SUPPLIER,\n          pK_USER: this.$store.state.currentUser.pk_user,\n          pK_WAREHOUSE: this.$store.state.currentUser.pk_warehouse,\n          details: this.details\n        };\n        await this.$store.dispatch('goodsreceipt/createReceipt', receiptData);\n        this.$emit('saved', {\n          ...this.localReceipt\n        });\n        this.close();\n      } catch (error) {\n        console.error('Error al guardar la entrada:', error);\n      }\n    },\n    updateDocuments() {\n      if (this.localReceipt.typE_RECEIPT === 'ADQUISICIÓN') {\n        this.localReceipt.typE_DOCUMENT = null, this.documentTypes = this.typesPurchases;\n      } else if (this.localReceipt.typE_RECEIPT === 'IMPORTACIÓN') {\n        this.localReceipt.typE_DOCUMENT = null, this.documentTypes = this.typesImports;\n      } else {\n        this.documentTypes = [];\n      }\n    },\n    uppercase() {\n      this.localReceipt.documenT_NUMBER = this.localReceipt.documenT_NUMBER.toUpperCase();\n      this.localReceipt.annotations = this.localReceipt.annotations.toUpperCase();\n    },\n    handleCheckProduct(product) {\n      const exists = this.details.some(item => item.pK_PRODUCT === product.pK_PRODUCT);\n      if (!exists) {\n        this.details.push({\n          pK_PRODUCT: product.pK_PRODUCT,\n          code: product.code,\n          description: product.description,\n          material: product.material,\n          colour: product.colour,\n          categorY_NAME: product.categorY_NAME,\n          branD_NAME: product.branD_NAME,\n          quantity: 0,\n          cost: 0\n        });\n        this.warningMessage = '';\n      } else {\n        this.warningMessage = 'El producto ya está en la lista.';\n      }\n    },\n    removeProduct(details) {\n      const index = this.details.indexOf(details);\n      if (index > -1) {\n        this.details.splice(index, 1);\n      }\n    },\n    openModal() {\n      this.modal = true;\n    },\n    close() {\n      this.isOpen = false;\n      this.warningMessage = '';\n      this.$emit('close');\n    }\n  },\n  mounted() {\n    this.details = [...this.receiptDetails];\n    this.$store.dispatch('supplier/selectSuppliers');\n    if (this.localReceipt.pK_RECEIPT == null) {\n      this.localReceipt.datE_PURCHASE = null;\n      this.updateDocuments();\n    } /* else {\r\n      const parts = this.localReceipt.datE_PURCHASE.split('/');\r\n      if (parts.length === 3) {\r\n        const day = parseInt(parts[0], 10);\r\n        const month = parseInt(parts[1], 10) - 1;\r\n        const year = parseInt(parts[2], 10);\r\n        const date = new Date(year, month, day);\r\n        this.localReceipt.datE_PURCHASE = date;\r\n      } else {\r\n        console.error('Formato de fecha no válido:', this.localReceipt.datE_PURCHASE);\r\n      }\r\n      } */\n  }\n};","map":{"version":3,"names":["mapGetters","ProductModal","jsPDF","autoTable","components","props","modelValue","type","Boolean","default","receipt","Object","pK_RECEIPT","code","datE_PURCHASE","datE_REGISTRATION","typE_RECEIPT","receipT_NUMBER","annotations","pK_SUPPLIER","pK_USER","pK_STORE","receiptDetails","Array","emits","data","details","itemCounter","items","pages","isOpen","alert","valid","modal","warningMessage","localReceipt","rules","required","value","documentTypes","typesPurchases","typesImports","receiptTypes","computed","headers","baseHeaders","title","key","sortable","push","totalCost","reduce","total","item","cost","watch","newValue","$emit","handler","newReceipt","deep","methods","printReceipt","date","Date","toLocaleString","doc","FONT_SIZE_TITLE","FONT_SIZE_SUBTITLE","TITLE_Y_POSITION","SUBTITLE_Y_POSITION","CODE_Y_POSITION","ENTRY_TYPE_Y_POSITION","TABLE_START_Y","PAGE_FOOTER_Y_POSITION","setTitle","text","y","setFontSize","setFont","setSubtitle","label","getTextWidth","documenT_NUMBER","rows","map","x","description","material","colour","categorY_NAME","branD_NAME","quantity","columns","header","dataKey","body","startY","margin","horizontal","styles","overflow","lineWidth","lineColor","bodyStyles","textColor","fillColor","font","valign","fontSize","headStyles","halign","footStyles","showHead","showFoot","columnStyles","pageCount","internal","getNumberOfPages","i","setPage","String","setProperties","window","open","output","saveReceipt","length","isFormValid","$refs","form","validate","requiredFields","typE_DOCUMENT","allFieldsValid","every","field","receiptData","$store","state","currentUser","pk_user","pK_WAREHOUSE","pk_warehouse","dispatch","close","error","console","updateDocuments","uppercase","toUpperCase","handleCheckProduct","product","exists","some","pK_PRODUCT","removeProduct","index","indexOf","splice","openModal","mounted"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\GoodsReceipt\\ReceiptForm.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <v-toolbar>\r\n      <v-toolbar-title>Entrada</v-toolbar-title>\r\n      <v-divider class=\"mx-2\" inset vertical></v-divider>\r\n      <div class=\"font-weight-bold\" style=\"font-size: 16px; margin-top: 4px; margin-right: 1620px;\">{{\r\n        this.localReceipt.code }} </div>\r\n      <v-spacer></v-spacer>\r\n    </v-toolbar>\r\n    <v-card v-if=\"isOpen\">\r\n      <v-card-text>\r\n        <v-form ref=\"form\" v-model=\"valid\">\r\n          <v-row>\r\n            <v-col cols=\"6\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-select v-if=\"localReceipt.pK_RECEIPT == null\" color=\"primary\" variant=\"underlined\"\r\n                v-model=\"localReceipt.typE_RECEIPT\" :items=\"receiptTypes\" label=\"Tipo de Entrada\"\r\n                :rules=\"[rules.required]\" @update:modelValue=\"updateDocuments\" />\r\n              <v-text-field v-else color=\"primary\" variant=\"underlined\" v-model=\"localReceipt.typE_RECEIPT\"\r\n                label=\"Tipo de Entrada\" readonly />\r\n            </v-col>\r\n            <v-col cols=\"6\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-select v-if=\"localReceipt.pK_RECEIPT == null\" color=\"primary\" variant=\"underlined\"\r\n                v-model=\"localReceipt.typE_DOCUMENT\" :items=\"documentTypes\" label=\"Tipo de Comprobante\"\r\n                :rules=\"[rules.required]\" />\r\n              <v-text-field v-else color=\"primary\" variant=\"underlined\" v-model=\"localReceipt.typE_DOCUMENT\"\r\n                label=\"Tipo de Comprobante\" readonly />\r\n            </v-col>\r\n            <v-col cols=\"6\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-date-input v-if=\"localReceipt.pK_RECEIPT == null\" locale=\"es\" placeholder=\"dd/mm/yyyy\"\r\n                v-model=\"localReceipt.datE_PURCHASE\" label=\"Fecha de Compra\" variant=\"underlined\" prepend-icon=\"\"\r\n                :rules=\"[rules.required]\" />\r\n              <v-text-field v-else v-model=\"localReceipt.datE_PURCHASE\" label=\"Fecha de Compra\" variant=\"underlined\"\r\n                readonly />\r\n            </v-col>\r\n            <v-col cols=\"6\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-text-field v-if=\"localReceipt.pK_RECEIPT == null\" color=\"primary\" variant=\"underlined\"\r\n                v-model=\"localReceipt.documenT_NUMBER\" :rules=\"[rules.required]\" counter=\"30\" :maxlength=\"30\"\r\n                label=\"Número de Comprobante\" @keyup=\"uppercase\" />\r\n              <v-text-field v-else color=\"primary\" variant=\"underlined\" v-model=\"localReceipt.documenT_NUMBER\"\r\n                label=\"Número de Comprobante\" readonly />\r\n            </v-col>\r\n            <v-col cols=\"6\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-autocomplete v-if=\"localReceipt.pK_RECEIPT == null\" color=\"primary\" variant=\"underlined\"\r\n                :items=\"suppliers\" v-model=\"localReceipt.pK_SUPPLIER\" item-title=\"companY_NAME\" item-value=\"pK_SUPPLIER\"\r\n                :rules=\"[rules.required]\" no-data-text=\"No hay datos disponibles\" label=\"Proveedor\" />\r\n              <v-text-field v-else color=\"primary\" variant=\"underlined\" v-model=\"localReceipt.companY_NAME\"\r\n                label=\"Proveedor\" readonly />\r\n            </v-col>\r\n            <v-col class=\"px-2\" cols=\"6\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-btn v-if=\"localReceipt.pK_RECEIPT == null\" fab dark color=\"blue darken-1\" class=\"mt-3\"\r\n                @click=\"openModal\">\r\n                <v-icon dark>list</v-icon>\r\n              </v-btn>\r\n            </v-col>\r\n          </v-row>\r\n        </v-form>\r\n        <v-divider></v-divider>\r\n        <v-data-table :headers=\"headers\" :items=\"details\" class=\"elevation-1\" hide-default-footer>\r\n          <template v-slot:item=\"{ item }\">\r\n            <tr>\r\n              <td>{{ item.code }}</td>\r\n              <td>{{ item.description }}</td>\r\n              <td>{{ item.material }}</td>\r\n              <td>{{ item.colour }}</td>\r\n              <td>{{ item.categorY_NAME }}</td>\r\n              <td>{{ item.branD_NAME }}</td>\r\n              <td v-if=\"localReceipt.pK_RECEIPT == null\"><v-text-field v-model.number=\"item.quantity\"\r\n                  variant=\"underlined\"\r\n                  onkeypress=\"return (event.charCode >= 48 && event.charCode <= 57)\"></v-text-field></td>\r\n              <td v-else>{{ item.quantity }}</td>\r\n              <td v-if=\"localReceipt.pK_RECEIPT == null\"><v-text-field v-model=\"item.cost\" variant=\"underlined\"\r\n                  onkeypress=\"return (event.charCode >= 48 && event.charCode <= 57)\"></v-text-field></td>\r\n              <td v-else>{{ item.cost }}</td>\r\n              <td v-if=\"localReceipt.pK_RECEIPT == null\">\r\n                <v-btn color=\"red\" icon=\"delete\" variant=\"text\" @click=\"removeProduct(item)\" size=\"small\"></v-btn>\r\n              </td>\r\n            </tr>\r\n          </template>\r\n        </v-data-table>\r\n        <v-col cols=\"12\" class=\"d-flex justify-end\">\r\n          <strong>Total Bs. </strong>{{ totalCost }}\r\n        </v-col>\r\n        <v-col cols=\"12\" md=\"12\" lg=\"12\" xl=\"12\">\r\n          <v-text-field color=\"primary\" variant=\"underlined\" label=\"Observaciones\" counter=\"80\" :maxlength=\"80\"\r\n            v-model=\"localReceipt.annotations\" @keyup=\"uppercase\"></v-text-field>\r\n        </v-col>\r\n        <v-alert v-if=\"alert\" type=\"warning\" class=\"mt-3\">\r\n          No hay productos en la lista. Por favor, añade productos.\r\n        </v-alert>\r\n      </v-card-text>\r\n      <v-col cols=\"12\" md=\"12\" lg=\"12\" xl=\"12\">\r\n        <v-card-actions>\r\n          <v-btn v-if=\"localReceipt.pK_RECEIPT == null\" color=\"blue\" dark class=\"mb-2\" elevation=\"4\"\r\n            @click=\"saveReceipt\" :disabled=\"!valid\">Guardar</v-btn>\r\n          <v-btn v-else color=\"blue\" dark class=\"mb-2\" elevation=\"4\" @click=\"printReceipt\">Imprimir</v-btn>\r\n          <v-btn color=\"red\" dark class=\"mb-2\" elevation=\"4\" @click=\"close\">Cancelar</v-btn>\r\n        </v-card-actions>\r\n      </v-col>\r\n    </v-card>\r\n    <ProductModal v-model=\"modal\" @update:modelValue=\"modal = $event\" @check-product=\"handleCheckProduct\"\r\n      :warningMessage=\"warningMessage\" @clear-warning=\"warningMessage = ''\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport ProductModal from '../Product/ProductModal.vue';\r\nimport { jsPDF } from 'jspdf'\r\nimport { autoTable } from 'jspdf-autotable'\r\nexport default {\r\n  components: {\r\n    ProductModal,\r\n  },\r\n  props: {\r\n    modelValue: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n    receipt: {\r\n      type: Object,\r\n      default: () => ({\r\n        pK_RECEIPT: null,\r\n        code: '',\r\n        datE_PURCHASE: null,\r\n        datE_REGISTRATION: '',\r\n        typE_RECEIPT: '',\r\n        receipT_NUMBER: '',\r\n        annotations: '',\