calBrand = { ...newBrand };\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n  methods: {\r\n    close() {\r\n      this.isOpen = false;\r\n      this.$emit('update:modelValue', false);\r\n    },\r\n    async enabled() {\r\n      try {\r\n        await this.$store.dispatch('brand/enabledBrand', this.localBrand.pK_BRAND);\r\n        this.toast.success('Marca habilitada con éxito!');\r\n        setTimeout(() => {\r\n          this.close();\r\n        }, 2000);\r\n      } catch (error) {\r\n        console.error(\"Error when enabling the brand:\", error.message);\r\n        this.toast.error('Error al guardar la marca.');\r\n      }\r\n    },\r\n    async disabled() {\r\n      try {\r\n        await this.$store.dispatch('brand/disabledBrand', this.localBrand.pK_BRAND);\r\n        this.toast.success('Marca deshabilitada con éxito!');\r\n        this.close();\r\n      } catch (error) {\r\n        console.error(\"Error when disabling the brand:\", error.message);\r\n        this.toast.error('Error al guardar la marca.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAsBA,SAASA,QAAO,QAAS,oBAAoB;AAC7C,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,KAAK,EAAE;MACLH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QACdC,QAAQ,EAAE,IAAI;QACdC,UAAU,EAAE;MACd,CAAC;IACH,CAAC;IACDC,MAAM,EAAE;MACNR,IAAI,EAAES,MAAM;MACZP,QAAQ,EAAE;IACZ;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAACZ,UAAU;MACvBa,KAAK,EAAE,KAAK;MACZC,KAAK,EAAEhB,QAAQ,CAAC,CAAC;MACjBiB,UAAU,EAAE;QAAE,GAAG,IAAI,CAACX;MAAM;IAC9B,CAAC;EACH,CAAC;EACDY,KAAK,EAAE;IACLhB,UAAUA,CAACiB,QAAQ,EAAE;MACnB,IAAI,CAACL,MAAK,GAAIK,QAAQ;IACxB,CAAC;IACDb,KAAK,EAAE;MACLc,OAAOA,CAACC,QAAQ,EAAE;QAChB,IAAI,CAACJ,UAAS,GAAI;UAAE,GAAGI;QAAS,CAAC;MACnC,CAAC;MACDC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,IAAI,CAACV,MAAK,GAAI,KAAK;MACnB,IAAI,CAACW,KAAK,CAAC,mBAAmB,EAAE,KAAK,CAAC;IACxC,CAAC;IACD,MAAMC,OAAOA,CAAA,EAAG;MACd,IAAI;QACF,MAAM,IAAI,CAA