{"ast":null,"code":"import { mapGetters } from 'vuex';\nexport default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    },\n    product: {\n      type: Object,\n      default: () => ({\n        pK_PRODUCT: null,\n        code: '',\n        description: '',\n        measurement: '',\n        material: '',\n        colour: '',\n        pK_CATEGORY: '',\n        pK_BRAND: ''\n      })\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localProduct: {\n        ...this.product\n      },\n      rules: {\n        required: value => !!value || 'Este campo es requerido.'\n      }\n    };\n  },\n  computed: {\n    ...mapGetters('category', ['categories']),\n    ...mapGetters('brand', ['brands'])\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    isOpen(newValue) {\n      this.$emit('update:modelValue', newValue);\n    },\n    product: {\n      handler(newProduct) {\n        this.localProduct = {\n          ...newProduct\n        };\n      },\n      deep: true\n    }\n  },\n  methods: {\n    uppercase() {\n      this.localProduct.code = this.localProduct.code.toUpperCase();\n      this.localProduct.description = this.localProduct.description.toUpperCase();\n      this.localProduct.measurement = this.localProduct.measurement.toUpperCase();\n      this.localProduct.material = this.localProduct.material.toUpperCase();\n      this.localProduct.colour = this.localProduct.colour.toUpperCase();\n    },\n    close() {\n      this.isOpen = false;\n    },\n    async saveProduct() {\n      if (this.$refs.form.validate()) {\n        try {\n          if (this.localProduct.pK_PRODUCT) {\n            await this.$product.dispatch('product/updateProduct', {\n              ...this.localProduct\n            });\n          } else {\n            console.log(this.localProduct);\n            await this.$product.dispatch('product/createProduct', {\n              ...this.localProduct\n            });\n          }\n          this.$emit('saved', {\n            ...this.localProduct\n          });\n          this.close();\n        } catch (error) {\n          console.error('Error al guardar el producto:', error);\n        }\n      }\n    },\n    mounted() {\n      this.$store.dispatch('category/fetchCategories');\n      this.$store.dispatch('brand/fetchBrands');\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","props","modelValue","type","Boolean","required","product","Object","default","pK_PRODUCT","code","description","measurement","material","colour","pK_CATEGORY","pK_BRAND","data","isOpen","valid","localProduct","rules","value","computed","watch","newValue","$emit","handler","newProduct","deep","methods","uppercase","toUpperCase","close","saveProduct","$refs","form","validate","$product","dispatch","console","log","error","mounted","$store"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\Product\\ProductForm.vue"],"sourcesContent":["<template>\r\n  <v-dialog v-model=\"isOpen\" max-width=\"500px\" persistent>\r\n    <v-card>\r\n      <v-card-title>\r\n        <span>{{ localProduct.pK_PRODUCTO ? 'Editar Producto' : 'Agregar Producto' }}</span>\r\n      </v-card-title>\r\n      <v-card-text>\r\n        <v-form ref=\"form\" v-model=\"valid\">\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"12\" xl=\"12\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localProduct.code\" :maxlength=\"25\"\r\n                  @keyup=\"uppercase\" label=\"Código\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localProduct.description\"\r\n                  :rules=\"[rules.required]\" :maxlength=\"50\" @keyup=\"uppercase\" label=\"Descripción\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localProduct.measurement\"\r\n                  :rules=\"[rules.required]\" :maxlength=\"15\" @keyup=\"uppercase\" label=\"Medida\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localProduct.material\" :maxlength=\"20\"\r\n                  @keyup=\"uppercase\" label=\"Material\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-text-field color=\"primary\" variant=\"underlined\" v-model=\"localProduct.colour\" :maxlength=\"20\"\r\n                  @keyup=\"uppercase\" label=\"Color\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-autocomplete color=\"primary\" variant=\"underlined\" :items=\"categories\"\r\n                  v-model=\"localProduct.pK_CATEGORY\" item-text=\"categorY_NAME\" item-value=\"pK_CATEGORY\" label=\"Categoría\" required />\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\" lg=\"6\" xl=\"6\">\r\n                <v-autocomplete color=\"primary\" variant=\"underlined\" :items=\"brands\" v-model=\"localProduct.pK_BRAND\"\r\n                  label=\"Marca\" required />\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-col xs12 sm12 md12 lg12 xl12>\r\n        <v-card-actions>\r\n          <v-btn color=\"blue\" dark class=\"mb-2\" @click=\"saveProduct\" :disabled=\"!valid\">Guardar</v-btn>\r\n          <v-btn color=\"red\" dark class=\"mb-2\" @click=\"close\">Cancelar</v-btn>\r\n        </v-card-actions>\r\n      </v-col>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n  \r\n  <script>\r\n  import { mapGetters } from 'vuex';\