{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data: () => ({\n    brands: [],\n    dialog: false,\n    dialogDelete: false,\n    search: \"\",\n    headers: [{\n      title: 'Descripción',\n      key: 'branD_NAME'\n    }, {\n      title: 'Acciones',\n      key: 'actions',\n      sortable: false\n    }],\n    editedIndex: -1,\n    editedItem: {\n      name: '',\n      calories: 0,\n      fat: 0,\n      carbs: 0,\n      protein: 0\n    },\n    defaultItem: {\n      name: '',\n      calories: 0,\n      fat: 0,\n      carbs: 0,\n      protein: 0\n    }\n  }),\n  computed: {\n    formTitle() {\n      return this.editedIndex === -1 ? 'New Item' : 'Edit Item';\n    }\n  },\n  watch: {\n    dialog(val) {\n      val || this.close();\n    },\n    dialogDelete(val) {\n      val || this.closeDelete();\n    }\n  },\n  created() {\n    this.initialize();\n    this.getall();\n  },\n  methods: {\n    getall() {\n      let me = this;\n      axios.get('https://localhost:7075/api/Brands/GetAllBrands').then(function (response) {\n        me.brands = response.data;\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    initialize() {},\n    editItem(item) {\n      this.editedIndex = this.desserts.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialog = true;\n    },\n    deleteItem(item) {\n      this.editedIndex = this.desserts.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialogDelete = true;\n    },\n    deleteItemConfirm() {\n      this.desserts.splice(this.editedIndex, 1);\n      this.closeDelete();\n    },\n    close() {\n      this.dialog = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    closeDelete() {\n      this.dialogDelete = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    save() {\n      if (this.editedIndex > -1) {\n        Object.assign(this.desserts[this.editedIndex], this.editedItem);\n      } else {\n        this.desserts.push(this.editedItem);\n      }\n      this.close();\n    }\n  }\n};","map":{"version":3,"names":["axios","data","brands","dialog","dialogDelete","search","headers","title","key","sortable","editedIndex","editedItem","name","calories","fat","carbs","protein","defaultItem","computed","formTitle","watch","val","close","closeDelete","created","initialize","getall","methods","me","get","then","response","catch","error","console","log","editItem","item","desserts","indexOf","Object","assign","deleteItem","deleteItemConfirm","splice","$nextTick","save","push"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\Brands.vue"],"sourcesContent":["<template>\r\n  <v-data-table :headers=\"headers\" :items=\"brands\" :search=\"search\" item-value=\"name\" class=\"elevation-1\">\r\n    <template v-slot:item=\"{ item }\">\r\n      <tr>\r\n        <td>\r\n\r\n            <span class=\"green--text\">{{ item.branD_NAME }}</span>\r\n        </td>\r\n        <td>\r\n          <v-icon class=\"me-2\" size=\"small\" @click=\"editItem(item)\">\r\n            mdi-pencil\r\n          </v-icon>\r\n          <v-icon size=\"small\" @click=\"deleteItem(item)\">\r\n            mdi-delete\r\n          </v-icon>\r\n        </td>\r\n      </tr>\r\n    </template>\r\n    <template v-slot:top>\r\n      <v-toolbar flat>\r\n        <v-toolbar-title>MARCAS</v-toolbar-title>\r\n        <!-- <v-divider class=\"mx-2\" inset vertical></v-divider> -->\r\n        <v-spacer></v-spacer>\r\n        <v-text-field append-inner-icon=\"mdi-magnify\" density=\"compact\" label=\"Búsqueda\" variant=\"solo\" hide-details\r\n          single-line v-model=\"search\"></v-text-field>\r\n        <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n          <template v-slot:acti