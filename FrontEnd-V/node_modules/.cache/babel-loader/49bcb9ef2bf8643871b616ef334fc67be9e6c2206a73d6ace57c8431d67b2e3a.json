{"ast":null,"code":"export default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      items: [5, 10, 25],\n      pages: \"Productos por Página\",\n      search: null,\n      isOpen: this.modelValue,\n      valid: false,\n      copiedWarehouses: []\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    }\n  },\n  computed: {\n    warehouses() {\n      const warehouses = this.$store.getters['warehouse/warehouses'];\n      this.copiedWarehouses = [...warehouses];\n      return warehouses;\n    },\n    loading() {\n      return this.$store.getters['warehouse/loading'];\n    },\n    headers() {\n      return [{\n        title: 'Código',\n        key: 'code'\n      }, {\n        title: 'Descripción',\n        key: 'description',\n        sortable: false\n      }, {\n        title: 'Material',\n        key: 'material',\n        sortable: false\n      }, {\n        title: 'Color',\n        key: 'colour',\n        sortable: false\n      }, {\n        title: 'Cantidad',\n        key: 'stock',\n        sortable: false\n      }, {\n        title: 'Precio',\n        key: 'price',\n        sortable: false\n      }, {\n        title: 'Categoría',\n        key: 'categoria',\n        sortable: false\n      }, {\n        title: 'Marca',\n        key: 'Marca',\n        sortable: false\n      }, {\n        title: 'Acciones',\n        key: 'actions',\n        sortable: false\n      }];\n    }\n  },\n  methods: {\n    initialize() {\n      this.search = null;\n    },\n    addroduct(product) {\n      this.selectedProduct = {\n        ...product\n      };\n    },\n    fetchStock() {\n      this.$store.dispatch('warehouse/fetchStock', {\n        text: this.search,\n        id: this.$store.state.currentUser.pk_store\n      });\n    },\n    close() {\n      this.isOpen = false;\n      this.$emit('update:modelValue', false);\n    }\n  }\n};","map":{"version":3,"names":["props","modelValue