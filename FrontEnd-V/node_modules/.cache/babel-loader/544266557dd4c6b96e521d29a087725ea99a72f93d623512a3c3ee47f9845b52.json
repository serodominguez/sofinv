{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport ReceiptForm from './ReceiptForm.vue';\nimport ReceiptAction from './ReceiptAction.vue';\nexport default {\n  components: {\n    ReceiptForm,\n    ReceiptAction\n  },\n  data() {\n    return {\n      items: [5, 10, 25],\n      pages: \"Entradas por Página\",\n      search: null,\n      form: false,\n      modal: false,\n      selectedReceipt: null\n    };\n  },\n  computed: {\n    ...mapGetters('goodsreceipt', ['receipts', 'loading']),\n    headers() {\n      return [{\n        title: 'Código',\n        key: 'code',\n        sortable: false\n      }, {\n        title: 'Fecha de Registro',\n        key: 'datE_REGISTRATION',\n        sortable: false\n      }, {\n        title: 'Fecha de Compra',\n        key: 'datE_PURCHASE',\n        sortable: false\n      }, {\n        title: 'Tipo de Entrada',\n        key: 'typE_RECEIPT',\n        sortable: false\n      }, {\n        title: 'Tipo de Comprobante',\n        key: 'typE_DOCUMENT',\n        sortable: false\n      }, {\n        title: 'Número de Comprobante',\n        key: 'receipT_NUMBER',\n        sortable: false\n      }, {\n        title: 'Proveedor',\n        key: 'companY_NAME',\n        sortable: false\n      }, {\n        title: 'Usuario',\n        key: 'useR_NAME',\n        sortable: false\n      }, {\n        title: 'Sucursal',\n        key: 'storE_NAME',\n        sortable: false\n      }, {\n        title: 'Estado',\n        key: 'status',\n        sortable: false\n      }, {\n        title: 'Acciones',\n        key: 'actions',\n        sortable: false\n      }];\n    }\n  },\n  methods: {\n    initialize() {\n      this.$store.dispatch('goodsreceipt/fetchReceipts');\n      this.search = null;\n    },\n    ...mapActions('goodsreceipt', ['fetchReceipts']),\n    openForm() {\n      this.selectedReceipt = {\n        pK_RECEIPT: null,\n        code: '',\n        datE_PURCHASE: '',\n        datE_REGISTRATION: '',\n        typE_RECEIPT: '',\n        typE_DOCUMENT: '',\n        documenT_NUMBER_NUMBER: '',\n        annotations: '',\n        pK_SUPPLIER: null,\n        pK_USER: null,\n        pK_STORE: null\n      };\n      this.form = true;\n    },\n    async displayReceipt(receipt) {\n      this.selectedReceipt = {\n        ...receipt\n      };\n      await this.$store.dispatch('goodsreceipt/fetchDetail', receipt.pK_RECEIPT);\n      const details = this.$store.getters['goodsreceipt/detailsReceipt'];\n      this.selectedReceipt.details = details || [];\n      this.form = true;\n    },\n    openModal(receipt) {\n      this.selectedReceipt = {\n        ...receipt\n      };\n      this.modal = true;\n    },\n    fetchReceipt() {\n      this.$store.dispatch('goodsreceipt/fetchReceipt', this.search);\n    }\n  },\n  mounted() {\n    this.fetchReceipts();\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","ReceiptForm","ReceiptAction","components","data","items","pages","search","form","modal","selectedReceipt","computed","headers","title","key","sortable","methods","initialize","$store","dispatch","openForm","pK_RECEIPT","code","datE_PURCHASE","datE_REGISTRATION","typE_RECEIPT","typE_DOCUMENT","documenT_NUMBER_NUMBER","annotations","pK_SUPPLIER","pK_USER","pK_STORE","displayReceipt","receipt","details","getters","openModal","fetchReceipt","mounted","fetchReceipts"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\GoodsReceipt\\ReceiptList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <template v-if=\"!form\">\r\n      <v-card elevation=\"2\">\r\n        <v-data-table :headers=\"headers\" :items=\"receipts\" :search=\"search\" :items-per-page-text=\"pages\"\r\n          :items-per-page-options=\"[5, 10, 20]\" :items-per-page=\"5\" :loading=\"loading\"\r\n          loading-text=\"Cargando... Espere por favor\" page-text=\"{0}-{1} de {2}\">\r\n          <template v-slot:item=\"{ item }\">\r\n            <tr>\r\n              <td>{{ item.code }}</td>\r\n              <td>{{ item.datE_REGISTRATION }}</td>\r\n              <td>{{ item.datE_PURCHASE }}</td>\r\n              <td>{{ item.typE_RECEIPT }}</td>\r\n              <td>{{ item.typE_DOCUMENT }}</td>\r\n              <td>{{ item.documenT_NUMBER }}</td>\r\n              <td>{{ item.companY_NAME }}</td>\r\n              <td>{{ item.useR_NAME }}</td>\r\n              <td>{{ item.storE_NAME }}</td>\r\n              <td>{{ item.status }}</td>\r\n              <td>\r\n            <v-btn color=\"blue\" icon=\"tab\" variant=\"text\" @click=\"displayReceipt(item)\" size=\"small\"></v-btn>\r\n            <template v-if=\"item.status == 'ACTIVO'\">\r\n              <v-btn color=\"red\" icon=\"block\" variant=\"text\" @click=\"openModal(item)\" size=\"small\"></v-btn>\r\n            </template>\r\n          </td>\r\n            </tr>\r\n          </template>\r\n          <template v-slot:top>\r\n            <v-toolbar>\r\n              <v-toolbar-title>Gestión de Entradas</v-toolbar-title>\r\n              <v-spacer></v-spacer>\r\n              <v-text-field append-inner-icon=\"search\" density=\"compact\" label=\"Búsqueda\" variant=\"solo\" hide-details\r\n                single-line v-model=\"search\" @click:append-inner=\"fetchReceipt\" class=\"elevation-1\"></v-text-field>\r\n              <v-card-actions>\r\n                <v-btn @click=\"openForm\" color=\"primary\" elevation=\"4\"> Agregar </v-btn>\r\n              </v-card-actions>\r\n            </v-toolbar>\r\n          </template>\r\n          <template v-slot:no-data>\r\n            <v-btn color=\"primary\" @click=\"initialize\"> Resetear </v-btn>\r\n          </template>\r\n        </v-data-table>\r\n      </v-card>\r\n    </template>\r\n    <template v-else>\r\n      <ReceiptForm v-model=\"form\" :receipt=\"selectedReceipt\" :receiptDetails=\"selectedReceipt.details\"\r\n        @saved=\"fetchReceipts\" @close=\"form = false\" />\r\n    </template>\r\n    <ReceiptAction v-model=\"modal\" :receipt=\"selectedReceipt\" @update:modelValue=\"modal = $event\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from 'vuex';\r\nimport ReceiptForm from './ReceiptForm.vue';\r\nimport ReceiptAction from './ReceiptAction.vue';\r\nexport default {\r\n  components: {\r\n    ReceiptForm,\r\n    ReceiptAction,\r\n  },\r\n  data() {\r\n    return {\r\n      items: [5, 10, 25],\r\n      pages: \"Entradas por Página\",\r\n      search: null,\r\n      form: false,\r\n      modal: false,\r\n      selectedReceipt: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters('goodsreceipt', ['receipts', 'loading']),\r\n    headers() {\r\n      return [\r\n        { title: 'Código', key: 'code', sortable: false },\r\n        { title: 'Fecha de Registro', key: 'datE_REGISTRATION', sortable: false },\r\n        { title: 'Fecha de Compra', key: 'datE_PURCHASE', sortable: false },\r\n        { title: 'Tipo de Entrada', key: 'typE_RECEIPT', sortable: false },\r\n        { title: 'Tipo de Comprobante', key: 'typE_DOCUMENT', sortable: false },\r\n        { title: 'Número de Comprobante', key: 'receipT_NUMBER', sortable: false },\r\n        { title: 'Proveedor', key: 'companY_NAME', sortable: false },\r\n        { title: 'Usuario', key: 'useR_NAME', sortable: false },\r\n        { title: 'Sucursal', key: 'storE_NAME', sortable: false },\r\n        { title: 'Estado', key: 'status', sortable: false },\r\n        { title: 'Acciones', key: 'actions', sortable: false },\r\n      ];\r\n    },\r\n  },\r\n  methods: {\r\n    initialize() {\r\n      this.$store.dispatch('goodsreceipt/fetchReceipts');\r\n      this.search = null;\r\n    },\r\n    ...mapActions('goodsreceipt', ['fetchReceipts']),\r\n    openForm() {\r\n      this.selectedReceipt = {\r\n        pK_RECEIPT: null, code: '',\r\n        datE_PURCHASE: '',\r\n        datE_REGISTRATION: '',\r\n        typE_RECEIPT: '',\r\n        typE_DOCUMENT: '',\r\n        documenT_NUMBER_NUMBER: '',\r\n        annotations: '',\r\n        pK_SUPPLIER: null,\r\n        pK_USER: null,\r\n        pK_STORE: null\r\n      };\r\n      this.form = true;\r\n    },\r\n    async displayReceipt(receipt) {\r\n      this.selectedReceipt = { ...receipt };\r\n      await this.$store.dispatch('goodsreceipt/fetchDetail', receipt.pK_RECEIPT);\r\n      const details = this.$store.getters['goodsreceipt/detailsReceipt'];\r\n      this.selectedReceipt.details = details || []\r\n      this.form = true;\r\n    },\r\n    openModal(receipt) {\r\n      this.selectedReceipt = { ...receipt };\r\n      this.modal = true;\r\n    },\r\n    fetchReceipt() {\r\n      this.$store.dispatch('goodsreceipt/fetchReceipt', this.search);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchReceipts();\r\n  },\r\n};\r\n</script>"],"mappings":"AAqDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,aAAY,MAAO,qBAAqB;AAC/C,eAAe;EACbC,UAAU,EAAE;IACVF,WAAW;IACXC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,CAAC;MAClBC,KAAK,EAAE,qBAAqB;MAC5BC,MAAM,EAAE,IAAI;MACZC,IAAI,EAAE,KAAK;MACXC,KAAK,EAAE,KAAK;MACZC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGZ,UAAU,CAAC,cAAc,EAAE,CAAC,UAAU,EAAE,SAAS,CAAC,CAAC;IACtDa,OAAOA,CAAA,EAAG;MACR,OAAO,CACL;QAAEC,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,MAAM;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACjD;QAAEF,KAAK,EAAE,mBAAmB;QAAEC,GAAG,EAAE,mBAAmB;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACzE;QAAEF,KAAK,EAAE,iBAAiB;QAAEC,GAAG,EAAE,eAAe;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACnE;QAAEF,KAAK,EAAE,iBAAiB;QAAEC,GAAG,EAAE,cAAc;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAClE;QAAEF,KAAK,EAAE,qBAAqB;QAAEC,GAAG,EAAE,eAAe;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACvE;QAAEF,KAAK,EAAE,uBAAuB;QAAEC,GAAG,EAAE,gBAAgB;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC1E;QAAEF,KAAK,EAAE,WAAW;QAAEC,GAAG,EAAE,cAAc;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC5D;QAAEF,KAAK,EAAE,SAAS;QAAEC,GAAG,EAAE,WAAW;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACvD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,YAAY;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,QAAQ;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACnD;QAAEF,