{"ast":null,"code":"export default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    },\n    receipt: {\n      type: Object,\n      default: () => ({\n        pK_RECEIPT: null,\n        code: '',\n        datE_PURCHASE: '',\n        datE_REGISTRATION: '',\n        typE_RECEIPT: '',\n        receipT_NUMBER: '',\n        annotations: '',\n        pK_SUPPLIER: null,\n        pK_USER: null,\n        pK_STORE: null\n      })\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localReceipt: {\n        ...this.receipt\n      },\n      rules: {\n        required: value => !!value || 'Este campo es requerido.'\n      }\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    isOpen(newValue) {\n      this.$emit('update:modelValue', newValue);\n    },\n    receipt: {\n      handler(newReceipt) {\n        this.localReceipt = {\n          ...newReceipt\n        };\n      },\n      deep: true\n    }\n  },\n  methods: {\n    close() {\n      this.isOpen = false; // Cierra el formulario\n      this.$emit('close'); // Emite el evento para que el componente padre lo maneje\n    },\n    async saveReceipt() {\n      if (this.$refs.form.validate()) {\n        try {\n          await this.$store.dispatch('goodsreceipt/createReceipt', {\n            ...this.localReceipt\n          });\n          this.$emit('saved', {\n            ...this.localReceipt\n          });\n          this.close();\n        } catch (error) {\n          console.error('Error al guardar la entrada:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["props","modelValue","type","Boolean","required","receipt","Object","default","pK_RECEIPT","code","datE_PURCHASE","datE_REGISTRATION","typE_RECEIPT","receipT_NUMBER","annotations","pK_SUPPLIER","pK_USER","pK_STORE","data","isOpen","valid","localReceipt","rules","value","watch","newValue","$emit","handler","newReceipt","deep","methods","close","saveReceipt","$refs","form","validate","$store","dispatch","error","console"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\GoodsReceipt\\ReceiptForm.vue"],"sourcesContent":["<template>\r\n  <v-card v-if=\"isOpen\">\r\n    <v-card-title class=\"bg-surface-light pt-4\">\r\n        <span>Entradas</span>\r\n      </v-card-title>\r\n      <v-divider></v-divider>\r\n    <v-card-text>\r\n      <v-form ref=\"form\" v-model=\"valid\">\r\n        <v-container>\r\n          <v-row>\r\n            <v-col cols=\"12\" md=\"2\" lg=\"2\" xl=\"2\">\r\n              <v-text-field \r\n              color=\"primary\" \r\n              variant=\"underlined\" \r\n              v-model=\"localReceipt.datE_PURCHASE\"\r\n              :rules=\"[rules.required]\"  \r\n              placeholder=\"Fecha\" \r\n              required />\r\n            </v-col>\r\n          </v-row>\r\n        </v-container>\r\n      </v-form>\r\n    </v-card-text>\r\n    <v-col xs12 sm12 md12 lg12 xl12>\r\n      <v-card-actions>\r\n        <v-btn color=\"blue\" dark class=\"mb-2\" elevation=\"4\" @click=\"saveReceipt\" :disabled=\"!valid\">Guardar</v-btn>\r\n        <v-btn color=\"red\" dark class=\"mb-2\" elevation=\"4\" @click=\"close\">Cancelar</v-btn>\r\n      </v-card-actions>\r\n    </v-col>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    modelValue: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n    receipt: {\r\n      type: Object,\r\n      default: () => ({ pK_RECEIPT: null, code: '', datE_PURCHASE: '', datE_REGISTRATION: '', typE_RECEIPT: '', receipT_NUMBER: '', annotations: '', pK_SUPPLIER: null, pK_USER: null, pK_STORE: null }),\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      isOpen: this.modelValue,\r\n      valid: false,\r\n      localReceipt: { ...this.receipt },\r\n      rules: {\r\n        required: (value) => !!value || 'Este campo es requerido.',\r\n      },\r\n    };\r\n  },\r\n  watch: {\r\n    modelValue(newValue) {\r\n      this.isOpen = newValue;\r\n    },\r\n    isOpen(newValue) {\r\n      this.$emit('update:modelValue', newValue);\r\n    },\r\n    receipt: {\r\n      handler(newReceipt) {\r\n        this.localReceipt = { ...newReceipt };\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n  methods: {\r\n    close() {\r\n      this.isOpen = false; // Cierra el formulario\r\n      this.$emit('close'); // Emite el evento para que el componente padre lo maneje\r\n    },\r\n    async saveReceipt() {\r\n      if (this.$refs.form.validate()) {\r\n        try {\r\n          await this.$store.dispatch('goodsreceipt/createReceipt', { ...this.localReceipt });\r\n          this.$emit('saved', { ...this.localReceipt });\r\n          this.close();\r\n        } catch (error) {\r\n          console.error('Error al guardar la entrada:', error);\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAiCA,eAAe;EACbA,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO;QAAEC,UAAU,EAAE,IAAI;QAAEC,IAAI,EAAE,EAAE;QAAEC,aAAa,EAAE,EAAE;QAAEC,iBAAiB,EAAE,EAAE;QAAEC,YAAY,EAAE,EAAE;QAAEC,cAAc,EAAE,EAAE;QAAEC,WAAW,EAAE,EAAE;QAAEC,WAAW,EAAE,IAAI;QAAEC,OAAO,EAAE,IAAI;QAAEC,QAAQ,EAAE;MAAK,CAAC;IACnM;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAAClB,UAAU;MACvBmB,KAAK,EAAE,KAAK;MACZC,YAAY,EAAE;QAAE,GAAG,IAAI,CAAChB;MAAQ,CAAC;MACjCiB,KAAK,EAAE;QACLlB,QAAQ,EAAGmB,KAAK,IAAK,CAAC,CAACA,KAAI,IAAK;MAClC;IACF,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLvB,UAAUA,CAACwB,QAAQ,EAAE;MACnB,IAAI,CAACN,MAAK,GAAIM,QAAQ;IACxB,CAAC;IACDN,MAAMA,CAACM,QAAQ,EAAE;MACf,IAAI,CAACC,KAAK,CAAC,mBAAmB,EAAED,QAAQ,CAAC;IAC3C,CAAC;IACDpB,OAAO,EAAE;MACPsB,OAAOA,CAACC,UAAU,EAAE;QAClB,IAAI,CAACP,YAAW,GAAI;UAAE,GAAGO;QAAW,CAAC;MACvC,CAAC;MACDC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,IAAI,CAACZ,MAAK,GAAI,KAAK,EAAE;MACrB,IAAI,CAACO,KAAK,CAAC,OAAO,CAAC,EAAE;IACvB,CA