ate>\r\n        <v-list-item v-for=\"transfer in linkTransfers\" :key=\"transfer.text\" :title=\"transfer.text\" :prepend-icon=\"transfer.icon\" :to=\"transfer.route\">\r\n          <v-list-item-content>\r\n            <v-list-item-title>{{ transfer.text }}</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list-group>\r\n      <v-list-group v-if=\"isAdministrator\" v-slot:activator=\"{ props }\">\r\n        <v-list-item v-bind=\"props\">\r\n          <v-list-item-title>Accesos</v-list-item-title>\r\n        </v-list-item>\r\n        <v-list-item v-for=\"link in linkAccess\" :key=\"link.text\" :title=\"link.text\" :prepend-icon=\"link.icon\" :to=\"link.route\">\r\n          <v-list-item-content>\r\n            <v-list-item-title>{{ link.text }}</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list-group>\r\n    </v-list>\r\n  </v-navigation-drawer>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Appbar\",\r\n  data: () => ({\r\n    drawer: false,\r\n    linkStore: [\r\n      { icon: 'add_shopping_cart', text: 'Entradas', route: '/receipts' },\r\n      { icon: 'category', text: 'Categorías', route: '/categories' },\r\n      { icon: 'account_box', text: 'Clientes', route: '/customers' },\r\n      { icon: 'copyright', text: 'Marcas', route: '/brands' },\r\n      { icon: 'inventory_2', text: 'Productos', route: '/products' },\r\n      { icon: 'contacts', text: 'Proveedores', route: '/suppliers' },\r\n      { icon: 'remove_shopping_cart', text: 'Salidas', route: '/issues' },\r\n      { icon: 'home_work', text: 'Sucursales', route: '/stores' }\r\n    ],\r\n    linkTransfers: [\r\n      { icon: 'swap_horiz', text: 'Traspasos Entrantes', route: '/transfers/incoming' },\r\n      { icon: 'swap_vert', text: 'Traspasos Salientes', route: '/transfers/outgoing' },\r\n    ],\r\n    linkAccess: [\r\n      { icon: 'manage_accounts', text: 'Roles', route: '/roles' },\r\n      { icon: 'person', text: 'Usuarios', route: '/users' },\r\n    ],\r\n  }),\r\n  computed: {\r\n    loggedin() {\r\n      return this.$store.state.currentUser ;\r\n    },\r\n    isAdministrator() {\r\n      return this.loggedin && this.loggedin.role === \"ADMINISTRADORES\";\r\n    },\r\n    isGrocer() {\r\n      return this.loggedin && this.loggedin.role === \"ALMACENEROS\";\r\n    },\r\n    isOperator() {\r\n      return this.loggedin && this.loggedin.role === \"OPERARIOS\";\r\n    }\r\n  },\r\n  created() {\r\n    this.$store.dispatch(\"auto\");\r\n  },\r\n  methods: {\r\n    logoff() {\r\n      this.$store.dispatch(\"logoff\");\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Ajustes de estilo si es necesario */\r\n.v-list-group {\r\n  margin-left: 16px; /* Ajusta este valor según sea necesario */\r\n}\r\n</style>"],"mappings":"AAyDA,eAAe;EACbA,IAAI,EAAE,QAAQ;EACdC,IAAI,EAAEA,CAAA,MAAO;IACXC,MAAM,EAAE,KAAK;IACbC,SAAS,EAAE,CACT;MAAEC,IAAI,EAAE,mBAAmB;MAAEC,IAAI,EAAE,UAAU;MAAEC,KAAK,EAAE;IAAY,CAAC,EACnE;MAAEF,IAAI,EAAE,UAAU;MAAEC,IAAI,EAAE,YAAY;MAAEC,KAAK,EAAE;IAAc,CAAC,EAC9D;MAAEF,IAAI,EAAE,aAAa;MAAEC,IAAI,EAAE,UAAU;MAAEC,KAAK,EAAE;IAAa,CAAC,EAC9D;MAAEF,IAAI,EAAE,WAAW;MAAEC,IAAI,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAU,CAAC,EACvD;MAAEF,IAAI,EAAE,aAAa;MAAEC,IAAI,EAAE,WAAW;MAAEC,KAAK,EAAE;IAAY,CAAC,EAC9D;MAAEF,IAAI,EAAE,UAAU;MAAEC,IAAI,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAa,CAAC,EAC9D;MAAEF,IAAI,EAAE,sBAAsB;MAAEC,IAAI,EAAE,SAAS;MAAEC,KAAK,EAAE;IAAU,CAAC,EACnE;MAAEF,IAAI,EAAE,WAAW;MAAEC,IAAI,EAAE,YAAY;MAAEC,KAAK,EAAE;IAAU,EAC3D;IACDC,aAAa,EAAE,CACb;MAAEH,IAAI,EAAE,YAAY;MAAEC,IAAI,EAAE,qBAAqB;MAAEC,KAAK,EAAE;IAAsB,CAAC,EACjF;MAAEF,IAAI,EAAE,WAAW;MAAEC,IAAI,EAAE,qBAAqB;MAAEC,KAAK,EAAE;IAAsB,CAAC,CACjF;IACDE,UAAU,EAAE,CACV;MAAEJ,IAAI,EAAE,iBAAiB;MAAEC,IAAI,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAS,CAAC,EAC3D;MAAEF,IAAI,EAAE,QAAQ;MAAEC,IAAI,EAAE,UAAU;MAAEC,KAAK,EAAE;IAAS,CAAC;EAEzD,CAAC,CAAC;EACFG,QAAQ,EAAE;IACRC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,WAAU;IACrC,CAAC;IA