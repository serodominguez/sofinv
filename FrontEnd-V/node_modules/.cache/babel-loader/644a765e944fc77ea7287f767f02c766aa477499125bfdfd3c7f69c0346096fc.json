le: false },\r\n      ];\r\n    },\r\n  },\r\n  methods: {\r\n    initialize() {\r\n      this.$store.dispatch('customer/fetchCustomers');\r\n      this.search = null;\r\n    },\r\n    ...mapActions('customer', ['fetchCustomers']),\r\n    openForm() {\r\n      this.selectedCustomer = {\r\n        pK_CUSTOMER: null,\r\n        names: '',\r\n        lasT_NAMES: '',\r\n        identification: '',\r\n        phone: ''\r\n      };\r\n      this.form = true;\r\n    },\r\n    editCustomer(customer) {\r\n      this.selectedCustomer = { ...customer };\r\n      this.form = true;\r\n    },\r\n    openModal(customer, action) {\r\n      this.selectedCustomer = { ...customer };\r\n      this.action = action;\r\n      this.modal = true;\r\n    },\r\n    searchCustomer() {\r\n      this.$store.dispatch('customer/fetchCustomer', this.search);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchCustomers();\r\n  },\r\n};\r\n</script>"],"mappings":"AA6CA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,cAAa,MAAO,sBAAsB;AACjD,eAAe;EACbC,UAAU,EAAE;IACVF,YAAY;IACZC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,CAAC;MAClBC,KAAK,EAAE,qBAAqB;MAC5BC,MAAM,EAAE,IAAI;MACZC,IAAI,EAAE,KAAK;MACXC,KAAK,EAAE,KAAK;MACZC,gBAAgB,EAAE,IAAI;MACtBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGb,UAAU,CAAC,UAAU,EAAE,CAAC,WAAW,EAAE,SAAS,CAAC,CAAC;IACnDc,OAAOA,CAAA,EAAG;MACR,OAAO,CACL;QAAEC,KAAK,EAAE,SAAS;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACnD;QAAEF,KAAK,EAAE,WAAW;QAAEC,GAAG,EAAE,YAAY;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC1D;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,gBAAgB;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC3D;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACpD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,eAAe;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC5D;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,QAAQ;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACnD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,SAAS;QAAEC,QAAQ,EAAE;MAAM,CAAC,CACvD;IACH;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAA