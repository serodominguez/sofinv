      <v-card-actions>\r\n          <v-btn color=\"red\" dark class=\"mb-2\" elevation=\"4\" @click=\"close\">Cerrar</v-btn>\r\n        </v-card-actions>\r\n      </v-col>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from 'vuex';\r\nexport default {\r\n  props: {\r\n    modelValue: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      items: [5, 10, 25],\r\n      pages: \"Productos por Página\",\r\n      search: null,\r\n      isOpen: this.modelValue,\r\n    };\r\n  },\r\n  watch: {\r\n    modelValue(newValue) {\r\n      this.isOpen = newValue;\r\n    },\r\n  },\r\n  computed: {\r\n    ...mapGetters('warehouse', ['warehouses', 'loading']),\r\n    headers() {\r\n      return [\r\n        { title: 'Código', key: 'code' },\r\n        { title: 'Descripción', key: 'description', sortable: false },\r\n        { title: 'Material', key: 'material', sortable: false },\r\n        { title: 'Color', key: 'colour', sortable: false },\r\n        { title: 'Cantidad', key: 'stock', sortable: false },\r\n        { title: 'Precio', key: 'price', sortable: false },\r\n        { title: 'Categoría', key: 'categoria', sortable: false },\r\n        { title: 'Marca', key: 'Marca', sortable: false },\r\n        { title: 'Acciones', key: 'actions', sortable: false },\r\n      ];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions('warehouse', ['fetchStock']),\r\n    async load() {\r\n      try {\r\n        this.products = await this.fetchStock({\r\n          text: this.search,\r\n          id: this.$store.state.currentUser .pk_store,\r\n        });\r\n      } catch (error) {\r\n        console.error('Error loading products:', error);\r\n      }\r\n    },\r\n    addroduct(product) {\r\n      this.selectedProduct = { ...product };\r\n    },\r\n    close() {\r\n      this.isOpen = false;\r\n      this.$emit('update:modelValue', false);\r\n    },\r\n  },\r\n};\