se },\r\n        { title: 'Agregar', key: 'actions', sortable: false },\r\n      ];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions('warehouse', ['fetchStock']),\r\n    searchProduct() {\r\n      this.$store.dispatch('warehouse/fetchStock', {\r\n        text: this.search,\r\n        id: this.$store.state.currentUser.pk_store\r\n      });\r\n    },\r\n    addProduct(item) {\r\n    this.$emit('add-product', {\r\n      pK_PRODUCT: item.pK_PRODUCT,\r\n      code: item.code,\r\n      description: item.description,\r\n      material: item.material,\r\n      colour: item.colour,\r\n      categorY_NAME: item.categorY_NAME,\r\n      branD_NAME: item.branD_NAME,\r\n      quantity: 0,\r\n      cost:  0\r\n    });\r\n  },\r\n    close() {\r\n      this.isOpen = false;\r\n      this.search = null;\r\n      this.$store.commit('warehouse/SET_CLEAR');\r\n      this.$emit('update:modelValue', false);\r\n    },\r\n    uppercase() {\r\n      this.search = this.search.toUpperCase();\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAkDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,CAAC;MAClBC,KAAK,EAAE,sBAAsB;MAC7BC,MAAM,EAAE,IAAI;MACZC,MAAM,EAAE,IAAI,CAACR;IACf,CAAC;EACH,CAAC;EACDS,KAAK,EAAE;IACLT,UAAUA,CAACU,QAAQ,EAAE;MACnB,IAAI,CAACF,MAAK,GAAIE,QAAQ;IACxB;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGd,UAAU,CAAC,WAAW,EAAE,CAAC,YAAY,EAAE,SAAS,CAAC,CAAC;IACrDe,OAAOA,CAAA,EAAG;MACR,OAAO,CACL;QAAEC,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE;MAAO,CAAC,EAChC;QAAED,KAAK,EAAE,aAAa;QAAEC,GAAG,EAAE,aAAa;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAC7D;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,UAAU;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACvD;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,QAAQ;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAClD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EACpD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,OAAO;QAAEC,QAAQ,EAAE;MAAM,CAAC,EAClD;QAAEF,KAAK,EAAE,WAAW;QA