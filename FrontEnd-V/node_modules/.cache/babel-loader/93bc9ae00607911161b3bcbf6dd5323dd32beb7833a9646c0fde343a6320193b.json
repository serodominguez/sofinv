{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nexport default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      items: [5, 10, 25],\n      pages: \"Productos por Página\",\n      search: null,\n      isOpen: this.modelValue\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    }\n  },\n  computed: {\n    ...mapGetters('warehouse', ['warehouses', 'loading']),\n    headers() {\n      return [{\n        title: 'Código',\n        key: 'code'\n      }, {\n        title: 'Descripción',\n        key: 'description',\n        sortable: false\n      }, {\n        title: 'Material',\n        key: 'material',\n        sortable: false\n      }, {\n        title: 'Color',\n        key: 'colour',\n        sortable: false\n      }, {\n        title: 'Cantidad',\n        key: 'stock',\n        sortable: false\n      }, {\n        title: 'Precio',\n        key: 'price',\n        sortable: false\n      }, {\n        title: 'Categoría',\n        key: 'categorY_NAME',\n        sortable: false\n      }, {\n        title: 'Marca',\n        key: 'branD_NAME',\n        sortable: false\n      }, {\n        title: 'Agregar',\n        key: 'actions',\n        sortable: false\n      }];\n    }\n  },\n  methods: {\n    ...mapActions('warehouse', ['fetchStock']),\n    searchProduct() {\n      this.$store.dispatch('warehouse/fetchStock', {\n        text: this.search,\n        id: this.$store.state.currentUser.pk_store\n      });\n    },\n    addProduct(item) {\n      this.$emit('add-product', {\n        pK_PRODUCT: item.pK_PRODUCT,\n        code: item.code,\n        description: item.description,\n        material: item.material,\n        colour: item.colour,\n        categorY_NAME: item.categorY_NAME,\n        branD_NAME: item.branD_NAME,\n        quantity: 0,\n        cost: 0\n      });\n    },\n    close() {\n      this.isOpen = false;\n      this.search = null;\n   