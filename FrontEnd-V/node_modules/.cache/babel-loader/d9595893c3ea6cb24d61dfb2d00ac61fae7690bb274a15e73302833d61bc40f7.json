{"ast":null,"code":"export default {\n  props: {\n    modelValue: {\n      type: Boolean,\n      required: true\n    },\n    store: {\n      type: Object,\n      default: () => ({\n        pK_PRODUCT: null,\n        code: '',\n        description: '',\n        measurement: '',\n        material: '',\n        colour: '',\n        price: '',\n        pK_CATEGORY: '',\n        pK_BRAND: ''\n      })\n    },\n    action: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      isOpen: this.modelValue,\n      valid: false,\n      localStore: {\n        ...this.store\n      }\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.isOpen = newValue;\n    },\n    store: {\n      handler(newStore) {\n        this.localStore = {\n          ...newStore\n        };\n      },\n      deep: true\n    }\n  },\n  methods: {\n    close() {\n      this.isOpen = false;\n      this.$emit('update:modelValue', false);\n    },\n    async activar() {\n      await this.$store.dispatch('store/enabledStore', this.localStore.pK_STORE);\n      this.close();\n    },\n    async desactivar() {\n      await this.$store.dispatch('store/disabledStore', this.localStore.pK_STORE);\n      this.close();\n    }\n  }\n};","map":{"version":3,"names":["props","modelValue","type","Boolean","required","store","Object","default","pK_PRODUCT","code","description","measurement","material","colour","price","pK_CATEGORY","pK_BRAND","action","Number","data","isOpen","valid","localStore","watch","newValue","handler","newStore","deep","methods","close","$emit","activar","$store","dispatch","pK_STORE","desactivar"],"sources":["C:\\Users\\Sergio\\Desktop\\SOFINV\\FrontEnd\\src\\components\\Product\\ProductModal.vue"],"sourcesContent":["<template>\r\n    <v-dialog v-model=\"isOpen\" max-width=\"400px\">\r\n      <v-card>\r\n        <v-card-title class=\"headline\" v-if=\"action === 1\">Activar Item?</v-card-title>\r\n        <v-card-title class=\"headline\" v-if=\"action === 2\